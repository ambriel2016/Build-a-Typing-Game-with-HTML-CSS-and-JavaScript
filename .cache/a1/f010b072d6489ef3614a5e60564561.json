umn":0},"generated":{"line":219,"column":0}},{"source":"app.css","original":{"line":220,"column":0},"generated":{"line":220,"column":0}}],"sources":{"app.css":":root {\n    --brand-color: #e16365;\n    --grey: #f9f9f9;\n    --main-font-stack: 'Poppins', 'arial', sans-serif;\n    --base-font-size: 0.9em;\n    --base-font-size-addition: 0.5vw;\n}\n* {\n    box-sizing: border-box;\n    color: #424242;\n    margin: 0;\n}\n\nhtml {\n    font-size: calc(var(--base-font-size) + var(--base-font-size-addition));\n}\n\nbody,\nbutton {\n    font-family: var(--main-font-stack);\n    font-weight: 300;\n}\n\n.logo {\n    font-size: 3rem;\n    text-align: center;\n    font-weight: 600;\n    margin-bottom: 50px;\n}\n\n.accent {\n    color: var(--brand-color);\n}\n\n.theme--fancy {\n    --main-font-stack: 'Alex Brush', cursive;\n    --grey: lemonchiffon;\n}\n\nbody {\n    margin: 0;\n    background-color: var(--grey);\n}\n\n.container {\n    padding: 20px;\n    margin: 0 auto;\n    margin-top: 20px;\n    max-width: 800px;\n}\n.stack > * + * {\n    margin-top: 2rem;\n}\n\n#randomCharacter {\n    font-size: 200px;\n    margin: 20px;\n    line-height: 1;\n    color: var(--brand-color);\n    text-align: center;\n}\n\n.button {\n    padding: 10px 20px;\n    background-color: var(--brand-color);\n    color: white;\n    border: none;\n    border-radius: 5px;\n    cursor: pointer;\n}\n\n.button-lg {\n    font-size: 18px;\n}\n\n.button-wide {\n    width: 200px;\n    margin-bottom: 5px;\n}\n\n.button-link {\n    background-color: transparent;\n    color: var(--main-font-stack);\n    font-size: 18px;\n    width: auto;\n    border-radius: 0;\n    padding: 0;\n    padding-bottom: -1px;\n    box-sizing: border-box;\n}\n\n.button-link:hover {\n    text-decoration: underline;\n    transform: scale(1.02);\n}\n\nnav {\n    margin-bottom: 50px;\n    display: flex;\n    justify-content: space-between;\n}\n\n.nav-items {\n    display: flex;\n    justify-content: flex-end;\n    align-items: center;\n}\n\n.nav-brand {\n    flex-grow: 1;\n    font-size: 24px;\n}\n\n.nav-items > button {\n    margin-right: 20px;\n}\n\n.cta {\n    border: none;\n    display: block;\n    background: transparent;\n    font-size: 1.25rem;\n}\n\n.cta:hover {\n    text-decoration: underline;\n}\n\n.center {\n    display: block;\n    margin-left: auto;\n    margin-right: auto;\n}\n\n.game-title {\n    font-size: 4.25rem;\n    font-weight: 300;\n    text-align: center;\n}\n\n.game-window {\n    height: 75vh;\n    max-height: 500px;\n    display: grid;\n    align-items: center;\n    grid-template-rows: 50px 1fr 50px;\n    grid-template-columns:\n        [full-start] minmax(50px, auto) 1fr minmax(50px, auto)\n        [full-end];\n}\n.character {\n    grid-row: 2;\n    grid-column: full;\n    transition: opacity 0.1s ease-out;\n}\n.timer {\n    grid-row: 1;\n    grid-column: 1;\n    font-size: 1.5rem;\n    font-weight: 600;\n}\n.score {\n    display: flex;\n    justify-content: flex-end;\n    grid-column: 3 / 4;\n}\n\n.score p:last-child {\n    margin-left: 2ch;\n}\nbutton:hover {\n    cursor: pointer;\n}\n\n.hidden {\n    display: none !important;\n}\n\n.text-center {\n    text-align: center;\n}\n\nh2 {\n    margin-bottom: 30px;\n}\n\nh3 {\n    margin-bottom: 20px;\n}\n\n.form-input {\n    width: 200px;\n    padding: 10px;\n    font-size: 16px;\n    margin-bottom: 10px;\n}\n\n.form-center {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n}\n\n@media (max-width: 768px) {\n    /* :root {\n        --base-font-size: 0.75em;\n        --base-font-size-addition: 0.25vw;\n    } */\n\n    nav {\n        flex-direction: column;\n        align-items: center;\n    }\n\n    .nav-brand {\n        margin-bottom: 10px;\n        font-size: 32px;\n    }\n}\n"},"lineCount":220}},"error":null,"hash":"4a864587d32b0e5b54c0926c403ff978","cacheData":{"env":{}}}